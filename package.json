{
  "name": "gis-server",
  "version": "0.4.0",
  "description": "Server",
  "author": "Anas <bladerlaiga@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "16.x.x"
  },
  "imports": {
    "#root/*": "./*.js",
    "#lib/*": "./lib/*.js",
    "#schema/*": "./schema/*.json",
    "#model/*": "./database/models/*.js",
    "#util/*": "./util/*.js",
    "#middleware/*": "./middleware/*.js",
    "#api/*": "./routes/api/*.js",
    "#routes/*": "./routes/*.js"
  },
  "scripts": {
    "install": "echo 'hello'",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "dev": "node-dev --trace-warnings --notify=false --respawn=false --clear ./bin/www",
    "schema:scan": "node-dev ./lib/scheming.js",
    "schema:gen": "json2ts --cwd=schema/v1 -i schema/v1/*.json -o type/v1/*.d.ts",
    "migrate:gen": "sequelize-cli model:generate --name new --attributes pk:integer --underscored --debug --force",
    "migrate:up": "sequelize-cli db:migrate --from --debug",
    "migrate:undo": "sequelize-cli db:migrate:undo --name --debug",
    "migrate:seed:all": "npx sequelize-cli db:seed:all",
    "migrate:seed:undo": "npx sequelize-cli db:seed:undo",
    "migrate:seed:new": "npx sequelize-cli seed:generate --name new --debug"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.0.1",
    "casbin": "^5.15.2",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-winston": "^4.2.0",
    "helmet": "^5.1.0",
    "http-errors": "^2.0.0",
    "http-proxy": "^1.18.1",
    "json-schema-to-typescript": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "opossum": "^6.4.0",
    "otpauth": "^7.1.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "rate-limiter-flexible": "^2.3.7",
    "rotating-file-stream": "^3.0.4",
    "sequelize": "^6.21.2",
    "sequelize-json-schema": "^2.1.1",
    "systeminformation": "^5.12.1",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "node-dev": "^7.4.3",
    "pm2": "^5.2.0",
    "sequelize-cli": "^6.4.1"
  }
}
